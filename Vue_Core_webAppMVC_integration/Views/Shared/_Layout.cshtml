@using Vue_Core_webAppMVC_integration.Helpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Vue_Core_webAppMVC_integration</title>
    @Html.PreloadResources()
